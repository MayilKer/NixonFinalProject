                    <span>
                        <i class="fa-solid fa-chevron-left"></i>
                    </span>
                </div>
                <div class="col-lg-8">
                    <div class="scrolling-text">
                        <p>
                            <a href="">
                                @banner.Content
                            </a>
                        </p>
                    </div>
                </div>
                <div class="arrow-right">
                    <i class="fa-solid fa-chevron-right"></i>
